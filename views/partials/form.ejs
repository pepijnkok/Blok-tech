    </fieldset>

    <fieldset>
        <legend> Onderwerp </legend>
        <label for="vraag">Vragen?</label>
        <textarea name="vraag" id="vraag" required placeholder="Stel hier je vraag!"></textarea>

    </fieldset>
    <button type="submit">Verstuur</button>

</form>

